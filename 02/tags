bool	main.c	/^typedef enum { false, true } bool;$/;"	t	typeref:enum:__anon1	file:
false	main.c	/^typedef enum { false, true } bool;$/;"	e	enum:__anon1	file:
hello_world	main.c	/^SDL_Surface *hello_world = 0;$/;"	v
init	main.c	/^bool init()$/;"	f
load_media	main.c	/^bool load_media()$/;"	f
main	main.c	/^int main(int argc, char **argv)$/;"	f
screen_surface	main.c	/^SDL_Surface *screen_surface = 0;$/;"	v
sdl_close	main.c	/^void sdl_close()$/;"	f
true	main.c	/^typedef enum { false, true } bool;$/;"	e	enum:__anon1	file:
window	main.c	/^SDL_Window *window = 0;$/;"	v
